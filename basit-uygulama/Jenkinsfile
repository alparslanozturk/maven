pipeline {

  agent {
    docker {
      image 'maven:latest'
      # args '-v /root/.m2:/var/lib/jenkins/.m2'
    }
  }


  stages {
    stage('build') {
      steps {
        sh 'mvn -B -DskipTests clean package'
      }
    }
    stage('Deliver'){
      steps {
        sh 'deliver.sh'
      }
    }
  }

}
